@Library('opstree-library@buildpiper_infra')

def terraformbuildpiper = new org.opstree.ci.terraformCI.buildPiperInfraSetup()

node{
withCredentials([[
    $class: 'AmazonWebServicesCredentialsBinding',
    credentialsId: "AWS",
    accessKeyVariable: 'AWS_ACCESS_KEY_ID',
    secretKeyVariable: 'AWS_SECRET_ACCESS_KEY'
]]) {
      echo "this is ${env.AWS_ACCESS_KEY_ID}"
echo "this is ${env.AWS_SECRET_ACCESS_KEY}"
      }  
  terraformbuildpiper.call(configFilePath: "env/management/configuration")
}